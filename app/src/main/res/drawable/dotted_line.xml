<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="-600dp"
        android:right="-600dp">
        <rotate
            android:fromDegrees="90"
            android:visible="true">
            <shape
                android:shape="line">
                <stroke
                    android:color="#4CAF50"
                    android:width="1dp"
                    android:dashWidth="6dp"
                    android:dashGap="4dp" />
            </shape>
        </rotate>
    </item>
</layer-list>